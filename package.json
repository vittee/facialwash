{
  "name": "facialwash",
  "version": "0.2.0",
  "private": true,
  "dependencies": {
    "classnames": "^2.3.2",
    "express": "^4.17.1",
    "lodash": "^4.17.14",
    "overmind": "27.0.0",
    "overmind-react": "28.0.0",
    "polished": "^4.2.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-is": "^18.2.0",
    "socket.io": "4.5.2",
    "socket.io-client": "4.5.2",
    "splashy": "5.0.0",
    "styled-components": "^5.3.0"
  },
  "scripts": {
    "start": "run-p dev:*",
    "dev:react": "vite",
    "dev:server": "tsx watch --tsconfig tsconfig.server.json server/server.ts",
    "build": "run-s clean build:server build:react",
    "build:react": "tsc --build && vite build",
    "build:server": "tsc --project tsconfig.server.json && tsx scripts/package.ts",
    "build:image": "run-s build && docker build . -t vittee/facialwash",
    "clean": "rimraf build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/express": "^4.17.0",
    "@types/lodash": "^4.14.136",
    "@types/node": "16.11.17",
    "@types/react": "18.0.20",
    "@types/react-dom": "18.0.6",
    "@types/styled-components": "^5.1.26",
    "@vitejs/plugin-react": "^2.1.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "sass": "^1.54.9",
    "tsx": "^3.9.0",
    "typescript": "4.8",
    "vite": "^3.1.2",
    "vite-tsconfig-paths": "^3.5.0"
  },
  "pnpm": {
    "allowedDeprecatedVersions": {
      "querystring": "*"
    }
  },
  "proxy": "http://localhost:4000"
}
